		AutoBlockEvent = state
	end
})

task.spawn(function()
	local BlockEvent = game:GetService("ReplicatedStorage"):WaitForChild("CombatServiceRemotes"):WaitForChild("BlockEvent")
	while true do
		if AutoBlockEvent == true then
			repeat
				BlockEvent:FireServer(true)
				wait(0.1)
			until AutoBlockEvent == false
		end
		task.wait(1)
	end
end)


AutoOpenPVP = false
